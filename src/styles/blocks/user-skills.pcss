.user-skills {
    
    padding-left: 30rem;
    padding-top: 6.25rem;
    @include desktop() {
        padding-left: 27rem;
    }
    @include desktopMini() {
        padding-left: 20rem;
    }
    @include tablets() {
        padding-left: 0rem;
        display: flex;
        flex-direction: column;
        align-items: center;       
    }
    @include phones() {
        padding-top: 4.25rem;
    }

}

.user-skills__item {
    margin-bottom: 30px;

}

